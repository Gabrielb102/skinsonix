{
  "name": "skinsonix-app",
  "productName": "skinsonix-app",
  "version": "1.0.6",
  "description": "My Electron application description",
  "main": "dist-electron/main/main.js",
  "type": "module",
  "license": "UNLICENSED",
  "keywords": [
    "skinsonix",
    "electron",
    "vue",
    "vite"
  ],
  "author": {
    "name": "Gabriel Burgos",
    "email": "gabrielb102@gmail.com"
  },
  "build": {
    "appId": "com.skinsonix.app",
    "productName": "SkinSonix App",
    "files": [
      "dist-electron/**/*",
      "dist/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icons/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "SkinSonix App"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "gabrielb102",
        "repo": "skinsonix",
        "private": true,
        "releaseType": "release"
      }
    ]
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && electron-builder",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx .",
    "rebuild-serialport": "electron-rebuild -f -w serialport",
    "rebuild-sqlite": "electron-rebuild -f -w sqlite3"
  },
  "devDependencies": {
    "@primevue/themes": "^4.2.5",
    "@serialport/bindings-cpp": "^12.0.1",
    "@serialport/stream": "^12.0.0",
    "@tailwindcss/vite": "^4.0.8",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "electron": "33.2.1",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.57.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.8.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.31.0",
    "ts-node": "^10.9.2",
    "vite": "^5.4.11",
    "vite-plugin-electron": "^0.29.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "dependencies": {
    "@primeuix/themes": "^1.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.3.9",
    "pinia": "^3.0.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.1",
    "sequelize": "^6.37.6",
    "serialport": "^12.0.0",
    "sqlite3": "^5.1.7",
    "start": "^5.1.0",
    "tailwindcss": "^4.0.7",
    "tailwindcss-primeui": "^0.5.1",
    "uuid": "^11.1.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  }
}
